#include <stdio.h>
struct student
{
    int rollnumber;
    char name[15];
    char branch[12];
    float marks;
};
int main()
{
    struct student s1;
    printf("Enter Roll Number : ");
    scanf("%d",&s1.rollnumber);
    printf("Enter Name : ");
    fflush(stdin);
    gets(s1.name);
    printf("Enter Branch : ");
    gets(s1.branch);
    printf("Enter Marks : ");
    scanf("%f",&s1.marks);
    printf("Roll Number : %d\n", s1.rollnumber);
    printf("Name : %s\n", s1.name);
    printf("Branch : %s\n", s1.branch);
    printf("Marks : %f\n", s1.marks);
    return 0;
}
