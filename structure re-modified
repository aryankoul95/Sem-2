#include <stdio.h>
struct student
{
    int rollnumber;
    char name[15];
    char branch[12];
    float marks;
};
int main()
{
    struct student s1,s2;
    printf("Enter Roll Number of First Student : ");
    scanf("%d",&s1.rollnumber);
    printf("Enter Roll Number of Second Student : ");
    scanf("%d",&s2.rollnumber);
    printf("Enter Name Of Fist Student : ");
    printf("Enter Name Of Fist Student : ");
    fflush(stdin);
    gets(s1.name);
    gets(s2.name);
    printf("Enter Branch of First Student : ");
    gets(s1.branch);
    printf("Enter Branch of Second Student : ");
    gets(s2.branch);
    printf("Enter Marks of First Student : ");
    scanf("%f",&s1.marks);
    printf("Enter Marks of Second Student : ");
    scanf("%f",&s2.marks);
    printf("Roll Number of First Student : %d\n", s1.rollnumber);
    printf("Roll Number of Second Stdent : %d\n", s2.rollnumber);
    printf("Name Of First Student : %s\n", s1.name);
    printf("Name Of Second Student : %s\n", s2.name);
    printf("Branch Of First Student : %s\n", s1.branch);
    printf("Branch Of Second Student : %s\n", s2.branch);
    printf("Marks Of First Student : %f\n", s1.marks);
    printf("Marks Of Second Student : %f\n", s2.marks);
    return 0;
}
